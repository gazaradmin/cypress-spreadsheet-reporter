{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["ES2015", "dom"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "removeComments": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "types": ["cypress", "node"]
  },
  "include": ["src", "cypress/index.d.ts"],
  "exclude": ["dist", "node_modules", "cypress"],
  "isolatedModules": false,
  "allowJs": true,
  "noEmit": true
}
